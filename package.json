{
  "name": "proof-of-humanity-discord-bot",
  "version": "1.0.0",
  "description": "Discord bot to assign role to registered human on Proof of Humanity",
  "main": "index.js",
  "scripts": {
    "start": "yarn run start:dev",
    "start:dev": "yarn run frontend:dev && yarn run backend:dev",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "build": "yarn run frontend:build && yarn run backend:build",
    "backend:dev": "cross-env NODE_ENV=develop nodemon --ignore db/ --ignore src/frontend/ --ignore config/ --exec babel-node src/index.js",
    "backend:build": "babel src --out-dir dist",
    "frontend:dev": "craco start",
    "frontend:build": "craco build",
    "build:remove": "del dist && del build",
    "log:remove": "del log",
    "serve": "npm run start:prod",
    "test": "npm run test:eslint && npm run test:flow",
    "test:eslint": "eslint src/",
    "test:eslint-fix": "eslint src/ --fix",
    "test:flow": "flow src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Nolan Vanmoortel",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@craco/craco": "^6.1.2",
    "@redux-saga/core": "^1.1.3",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@walletconnect/web3-provider": "^1.4.1",
    "@web3-react/core": "^6.1.9",
    "antd": "^4.16.1",
    "async-mutex": "^0.3.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "craco-less": "^1.17.1",
    "discord.js": "^12.5.1",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eth-provider": "^0.6.1",
    "ethers": "^5.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.1",
    "heartbeats": "^5.0.1",
    "jsome": "^2.5.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.33",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "spdy": "^4.0.1",
    "web-vitals": "^1.1.2",
    "web3modal": "^1.9.3",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.8.3",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "flow-bin": "^0.147.0",
    "nodemon": "^2.0.7"
  },
  "private": true
}
